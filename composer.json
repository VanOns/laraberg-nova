{
    "name": "van-ons/laraberg-nova",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "van-ons/laraberg": "dev-dev",
        "laravel/nova": "^4.35"
    },
    "autoload": {
        "psr-4": {
            "VanOns\\LarabergNova\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VanOns\\LarabergNova\\LarabergNovaServiceProvider",
                "VanOns\\Laraberg\\LarabergServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.62",
        "phpstan/phpstan": "^2.1"
    },
    "scripts": {
        "fix": "vendor/bin/php-cs-fixer fix",
        "analyse": "vendor/bin/phpstan analyse"
    }
}
