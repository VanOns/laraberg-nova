        "resolve-url-loader": "^4.0.0",
        "sass": "^1.43.4",
        "sass-loader": "^12.3.0",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.2",
        "vue-loader": "^15.9.8",
        "vue-template-compiler": "^2.6.14"
    },
